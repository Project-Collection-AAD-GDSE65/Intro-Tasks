<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet-mapping>
        <servlet-name>DemoTask</servlet-name>
        <url-pattern>/demo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DemoTask</servlet-name>
        <servlet-class>lk.ijse.gdse65.demotwo.DemoTask</servlet-class>
        <init-param>
            <param-name>InitParam</param-name>
            <param-value>DemoParam</param-value>
        </init-param>

    </servlet>

    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>lk.ijse.gdse65.demotwo.Test</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <context-param>
        <param-name>num</param-name>
        <param-value>2</param-value>
    </context-param>
</web-app>
